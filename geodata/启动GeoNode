启动GeoNode
source /usr/share/virtualenvwrapper/virtualenvwrapper.sh

workon geonode_dev
cd /opt/geonode
./paver_dev.sh start_django

启动GeoNode-project
source /usr/share/virtualenvwrapper/virtualenvwrapper.sh

workon my_geonode
cd /opt/geonode-project/my_geonode/src
./paver_dev.sh start_django


